{
  "name": "pokexplorer-training",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "mock:concat": "node \"./mocks/concat-json.js\"",
    "mock:json": "json-server -p 8064 --routes mocks/mock-routes.json --watch mocks/data.json --middlewares mocks/middleware.js",
    "mock:serve": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --port 4200",
    "mock": "npm run mock:concat && concurrently --kill-others \"npm run mock:json\" \"npm run mock:serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ng-select/ng-select": "^9.0.2",
    "@ngrx/store": "^14.3.2",
    "bootstrap": "^5.1.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "concurrently": "^7.6.0",
    "jasmine-core": "~4.1.0",
    "json-concat": "^0.0.1",
    "json-server": "^0.17.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}
